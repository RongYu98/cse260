


public class Point implements Comparable{

    int x;
    int y;

    public Point( int x, int y ){
	this.x=x;
	this.y=y;
    }
    
    public int compareTo( Point o ){
	if ( x!=o.getX() ){
	    return x-o.getX();
	}
	return y-o.getY();
    }

    public int getX(){
	return x;
    }
    public int getY(){
	return y;
    }




}
